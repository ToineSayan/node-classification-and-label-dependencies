{
    "normalization": "global-L1",
    "binarize": true,

    "seed": 42,

    "normalization_trick": "normalize_D-0.5",
    "to_symmetric": true,
    "add_self_links": true,

    "optimizer": "adam",
    "decay_policy": [-1],

    "splitting_method": "pre_computed",

    "epochs": 1000,
    "early_stopping_type": "LossDecreaseCriterion",
    "early_stopping": 200,
    "iterations": 20,
    "k_num_splits": 10,

    "GMNN_architecture": true,
    "EM_loops": 10,
    "EM_epochs": 100,
    "save_models_during_EM_phases": true,
    "tau_p": 0.1,
    "draw": "smp",
    "use_features_p": true,
    "use_gold_p": true
}